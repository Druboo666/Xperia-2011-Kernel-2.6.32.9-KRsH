device=$1
if [ "$device" == "" ]; then
    echo "[!] Device parameter missing"
    exit 0
fi

ARCH=arm CROSS_COMPILE=/home/cosmicdan/android/android_prebuilt_toolchains/arm-eabi-linaro-4.7/bin/arm-eabi- make turbo_$device_defconfig
#ARCH=arm CROSS_COMPILE=/home/cosmicdan/android/android_prebuilt_toolchains/arm-eabi-4.4.3/bin/arm-eabi- make turbo_$device_defconfig
cp -r -f ./ramdisk___configs/$device/* ./ramdisk
